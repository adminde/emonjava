<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class entity-name="entity">
        <id name="timestamp" type="ScaleInteger">
            <column name="time" sql-type="int unsigned"/>
         </id>
        <property name="value" type="java.lang.Object">
            <column name="data" />
        </property>
        <!-- property name="code" type="java.lang.Byte">
            <column name="code" />
        </property -->
    </class>
</hibernate-mapping>